<html>
<head></head>
    <body>
    	<h1>pagesList.ejs <%= norrisNamespace%></h1>
        <p id="log"></p>
        <script src="https://cdn.socket.io/socket.io-1.3.4.js"></script>
        <script> 
        var socket = io.connect('<%= fullURL%>'); 
        socket.on("configPageList", function (data) {
            console.log(JSON.stringify(data)); 
            document.getElementById("log").innerHTML+=JSON.stringify(data)+"<br />";
            //socket.emit("receivedMessage", data);
        });
        socket.on("news", function (data) {
            console.log(JSON.stringify(data)); 
            document.getElementById("log").innerHTML+=JSON.stringify(data)+"<br />";
            //socket.emit("receivedMessage", data);
        });
        </script>
    </body>
</html>